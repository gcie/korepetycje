{
  "name": "korepetycje",
  "version": "1.3.0",
  "scripts": {
    "start": "ng serve",
    "format": "prettier --config .prettierrc --write **/*.{js,ts,html,css,scss,json}",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "emulators": "firebase emulators:start --import=.emulators --export-on-exit",
    "deploy": "ng build --prod && firebase deploy",
    "deploy:webapp": "ng build --prod && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions"
  },
